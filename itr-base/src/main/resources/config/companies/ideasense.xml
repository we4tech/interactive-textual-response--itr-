  <Service-welcome-message>
    <echo><![CDATA[
  Welcome to IDEASense ITR service.
  Right now - ${time} and today - {$date}.
  ]]></echo>
  </Service-welcome-message>

  <!-- List of available services -->
  <Service-index>
