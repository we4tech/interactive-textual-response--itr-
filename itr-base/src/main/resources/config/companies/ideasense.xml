    <Index keyCode="2" title="Test recursive service.">
      <Index keyCode="a" title="Node 2.1"/>
      <Index keyCode="b" title="Node 2.2">
        <Index keyCode="i" title="Node 2.2.1">
          <echo>
            Test message.
          </echo>
        </Index>
      </Index>
      <Index keyCode="c" title="Node 2.3">
        <plug-in>http://localhost/test.php</plug-in>
      </Index>
    </Index>
  </Service-index>
</Navigation>